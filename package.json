{
  "name": "nbkt-api-core",
  "version": "0.0.1",
  "private": true,
  "description": "API Service",
  "main": "app.js",
  "scripts": {
    "test": "NODE_ENV=development yarn run test:unit",
    "test:unit": "mocha -r ts-node/register './src/**/*.spec.ts' --timeout 10000",
    "test:unit:watch": "mocha -r ts-node/register './src/**/*.spec.ts' --timeout 10000 --watch",
    "test:integration": "mocha -r ts-node/register './src/**/*.test.ts' --timeout 10000",
    "scripts:setup": "chmod +x scripts/docker/compose.sh scripts/docker/attach_shell.sh",
    "compose:build": "DOCKER_RUN_BUILD=YES NODE_ENV=development scripts/docker/compose.sh",
    "compose:up": "DOCKER_RUN_LAUNCH=YES NODE_ENV=development scripts/docker/compose.sh",
    "migrate:make": "NODE_ENV=migrate node_modules/knex/bin/cli.js migrate:make $MIGRATION_NAME -x ts",
    "migrate:latest": "node_modules/knex/bin/cli.js migrate:latest",
    "migrate:rollback": "node_modules/knex/bin/cli.js migrate:rollback",
    "start:dev": "nodemon -L ./bin/www",
    "build:dev": "tsc --watch --preserveWatchOutput",
    "dev": "concurrently \"npm:build:dev\" \"npm:start:dev\"",
    "build": "tsc",
    "lint": "node_modules/.bin/eslint . --ext js",
    "lint:fix": "node_modules/.bin/eslint . --ext js --fix"
  },
  "author": "buchanora",
  "license": "ISC",
  "devDependencies": {
    "@types/amqplib": "^0.5.17",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.9",
    "@types/express": "^4.17.11",
    "@types/knex": "^0.16.1",
    "@types/mocha": "^8.2.0",
    "@types/morgan": "^1.9.2",
    "@types/nodemailer": "^6.4.4",
    "@types/redis": "^2.8.28",
    "babel-eslint": "^10.0.1",
    "chai": "^3.5.0",
    "chai-http": "^3.0.0",
    "concurrently": "^3.5.0",
    "eslint": "^5.5.0",
    "eslint-cli": "^1.1.1",
    "mocha": "^3.1.2",
    "nodemon": "^1.12.1",
    "path": "^0.12.7",
    "strip": "^3.0.0",
    "ts-node": "^9.1.1",
    "typescript": "^3.7.4"
  },
  "dependencies": {
    "@hapi/joi": "^15.1.0",
    "@supabase/realtime-js": "^1.1.3",
    "amqplib": "^0.5.2",
    "bluebird": "^3.5.1",
    "body-parser": "~1.18.2",
    "bunyan": "^1.8.12",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.4",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.15.5",
    "express-validator": "^3.2.1",
    "jsonwebtoken": "^8.3.0",
    "jssha": "^2.3.1",
    "knex": "^0.21.17",
    "lodash": "^4.17.4",
    "md5": "^2.2.1",
    "moment": "^2.18.1",
    "morgan": "~1.9.0",
    "multer": "^1.3.0",
    "nodemailer": "^6.7.0",
    "pg": "^8.3.0",
    "pug": "^3.0.2",
    "redis": "^3.1.2",
    "serve-favicon": "~2.4.5",
    "sinon": "^1.14.1",
    "superagent": "^3.8.0",
    "uuid": "^3.1.0"
  }
}
